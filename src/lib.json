{
  "$schema": "https://json.schemastore.org/prettierrc",
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "useTabs": false,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-packagejson"],
  "overrides": [
    {
      "files": "package.json",
      "options": {
        "parser": "json-stringify"
      }
    }
  ]
}
